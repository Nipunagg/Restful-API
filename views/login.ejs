<%-include('./partials/header2')-%>
<body>
  <link rel='stylesheet' type="text/css" href="file.css">
  <div class="container">
    <div class="row">
      <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
        <div class="card card-signin my-5">
          <div class="card-body">
            <h5 class="card-title text-center">Register</h5>
            <form class="form-signin" method="post">
              <div class="form-label-group">
                <input type="email" name="email" class="form-control" placeholder="Email address" required autofocus>
                <label for="inputEmail">Email address</label>
              </div>

              <div class="form-label-group">
                <input type="password" name="password" class="form-control" placeholder="Password" required>
                <label for="inputPassword">Password</label>
              </div>

              <div class="custom-control custom-checkbox mb-3">
                <input type="checkbox" class="custom-control-input" id="customCheck1">
                <label class="custom-control-label" for="customCheck1">Remember password</label>
              </div>
              <button class="btn btn-lg btn-primary btn-block text-uppercase" formaction="/check" type="submit">Sign in</button>
              <button class="btn btn-lg btn-primary btn-block text-uppercase" formaction="/" type="submit">Register</button>
    
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
<%-include('./partials/footer')-%>